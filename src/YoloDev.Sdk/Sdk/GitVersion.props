<Project>

  <PropertyGroup>
    <NoWarn>$(NoWarn);NU5105;CS7035;CS1591</NoWarn>
    <GitVersionEnable Condition="'$(GitVersionEnable)' == ''">true</GitVersionEnable>
    <GitVersionForcePrerelease Condition="'$(GitVersionForcePrerelease)' == ''">false</GitVersionForcePrerelease>
  </PropertyGroup>

  <Choose>
    <When Condition="
      '$(GitVersion_FullSemVer)' != '' and
      '$(GitVersion_AssemblySemFileVer)' != '' and
      '$(GitVersion_InformationalVersion)' != ''
    ">
      <PropertyGroup>
        <_GitVersionSemVer>$(GitVersion_FullSemVer)</_GitVersionSemVer>
        <_GitVersionAssemblyVer>$(GitVersion_AssemblySemFileVer)</_GitVersionAssemblyVer>
        <_GitVersionInformational>$(GitVersion_InformationalVersion)</_GitVersionInformational>
        <_GitVersionSemVerPreRelease Condition="$(_GitVersionSemVer.Contains('-'))">$(_GitVersionSemVer)</_GitVersionSemVerPreRelease>
        <_GitVersionSemVerPreRelease Condition="!$(_GitVersionSemVer.Contains('-'))">$(_GitVersionSemVer)-pre.0</_GitVersionSemVerPreRelease>
      </PropertyGroup>
    </When>

    <Otherwise>
      <PropertyGroup>
        <_GitVersionSemVer>0.0.0-local.0</_GitVersionSemVer>
        <_GitVersionAssemblyVer>0.0.0.0</_GitVersionAssemblyVer>
        <_GitVersionInformational>0.0.0-local.0+dirty</_GitVersionInformational>
        <_GitVersionSemVerPreRelease>$(_GitVersionSemVer)</_GitVersionSemVerPreRelease>
      </PropertyGroup>
    </Otherwise>
  </Choose>

</Project>
